{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strictNullChecks": true
  }
}
